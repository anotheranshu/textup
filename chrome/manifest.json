{
    "manifest_version": 2,

    "name": "TextUp",
    "description": "Select text and send yourself a text.",
    "version": "1.0",

    "permissions": [
        "contextMenus",
         "tabs",
        "http://*/*",
        "https://*/*"
    ],

    "background": {
        "scripts": ["jquery-1.9.1.min.js", "main.js"]
    },

    "browser_action": {
    "default_icon": "icon-128.png",
    "default_title": "TextUp",
    "default_popup": "popup.html"
    },

    "icons": {
     "16": "icon-16.png",
     "48": "icon-48.png",
     "128": "icon-128.png" }
}
